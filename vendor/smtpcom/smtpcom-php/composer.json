{
    "name": "smtpcom/smtpcom-php",
    "description": "Smtp.com SDK provides methods for all API functions.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "beberlei/assert": "^3.3",
        "php-http/client-common": "^2.4.0",
        "php-http/discovery": "^1.9.1",
        "psr/http-client": "^1.0.1",
        "nyholm/psr7": "^1.4",
        "kriswallsmith/buzz": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0.0",
        "squizlabs/php_codesniffer": "^3.6.0",
        "php-http/guzzle7-adapter": "^1.0.0",
        "psalm/phar": "^4.9",
        "phpstan/phpstan": "^0.12.98"
    },
    "autoload": {
        "psr-4": {
            "SmtpSdk\\": "src/",
